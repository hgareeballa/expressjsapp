<!-- Body -->

<head>
    <meta charset="UTF-8">
    <title>Basic CRUD Application - jQuery EasyUI CRUD Demo</title>
    <link rel="stylesheet" type="text/css" href="/css/themes/material-blue/easyui.css">
    <link rel="stylesheet" type="text/css" href="/css/themes/icon.css">
    <link rel="stylesheet" type="text/css" href="/css/themes/color.css">        
</head>
<h2><%=title %></h2>
<table id="dg" title="<%=name %> View" class="easyui-datagrid" style="width:100%;height:500px" url="<%=loadurl %>"
     pagination="true" rownumbers="true" fitColumns="true" singleSelect="true">
    <thead>
        <tr>
            <% fields.forEach(function(field){ %>
            <th field="<%=field.id %>" width="50"><%=field.name %></th>
            <% }); %>
        </tr>
    </thead>
</table>
<script type="text/javascript" src="/js/jquery.easyui.min.js"></script>
